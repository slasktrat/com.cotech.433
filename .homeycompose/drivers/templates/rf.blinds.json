{
  "class": "windowcoverings",
  "capabilities": [
    "windowcoverings_state"
  ],
  "pair": [
    {
      "$template": "rf.imitate",
      "id": "rf.imitate",
      "options": {
        "body": "rf.pair.imitate.body.blinds"
      }
    },
    {
      "$template": "rf.test_blinds",
      "id": "rf.test_blinds",
      "navigation": {
        "prev": "rf.imitate",
        "next": "rf.done"
      }
    },
    {
      "$template": "rf.done",
      "id": "rf.done"
    }
  ]
}
